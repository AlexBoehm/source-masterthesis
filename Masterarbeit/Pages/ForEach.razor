@page "/foreach"

<h1>Lists</h1>

<ul>
    @foreach (var name in names) {
        <li>@name</li>
    }
</ul>

@code {
    List<string> names = new List<string>{
        "George Washington",
        "John Adams",
        "Thomas Jefferson",
        "James Madison",
        "James Monroe",
        "John Quincy Adams",
        "Andrew Jackson"
    };
}